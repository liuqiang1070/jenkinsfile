pipeline {
   agent any

   stages {
      stage('pull code') {
         steps {
            
         checkout([$class: 'GitSCM', branches: [[name: '*/${branch}']], doGenerateSubmoduleConfigurations: false, extensions: [], submoduleCfg: [], userRemoteConfigs: [[credentialsId: '2c87a10c-2730-4bc3-bc6e-c1723fdc4660', url: 'https://github.com/liuqiang1070/Hello.git']]])
        }  
             
    }
      
     stage('build') {
         steps {
           sh 'mvn clean package'
         }
      }
      
      stage('deploy') {
         steps {
            deploy adapters: [tomcat8(credentialsId: 'e2d61ab7-4887-4118-b132-2456bae06451', path: '', url: 'http://192.168.1.102:8080')], contextPath: null, war: 'target/*.war'

         }
      }
      
   }
}
